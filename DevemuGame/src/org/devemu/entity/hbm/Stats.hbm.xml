<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class catalog="ishina" entity-name="org.ishina.entity.Stats"
        name="org.ishina.entity.Stats" table="Stats">
        <id name="id" type="int">
            <column name="id"/>
            <generator class="assigned"/>
        </id>
        <property generated="never" name="statsId" type="int">
            <column length="11" name="statsId"/>
        </property>
        <property generated="never" name="value" type="int">
            <column length="11" name="value"/>
        </property>
    </class>
</hibernate-mapping>