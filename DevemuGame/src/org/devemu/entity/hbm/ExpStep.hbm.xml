<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class catalog="ishina" entity-name="org.ishina.entity.ExpStep"
        name="org.ishina.entity.ExpStep" table="ExpSteps">
        <id name="id" type="int">
            <column name="id"/>
            <generator class="assigned"/>
        </id>
        <property generated="never" name="player" type="long">
            <column length="20" name="player"/>
        </property>
        <property generated="never" name="alignement" type="long">
            <column length="20" name="alignement"/>
        </property>
    </class>
</hibernate-mapping>